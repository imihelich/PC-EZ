@model List<User>

<h1>Registered Users</h1>

<body>
    @foreach (LCCapstone.Models.User user in Model)
    {
        <a href="/User?username=@user.Username">@user.Username</a><br />
    }

    @if (Model.Count == 0)
    {
        <p> No registered users yet.</p>
    }
    <br />
</body>

<p><a asp-controller="User" asp-action="Add">Register</a></p>